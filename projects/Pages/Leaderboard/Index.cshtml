@page
@model projects.Pages.Leaderboard.IndexModel
@{
    ViewData["Title"] = "Leaderboard";
}
<h1>Leaderboard</h1>
<table class="table">
    <thead>
        <tr><th>User</th><th>Wins</th></tr>
    </thead>
    <tbody>
    @foreach (var row in Model.Rows)
    {
        <tr>
            <td>@row.User.DisplayName ?? row.User.UserName</td>
            <td>@row.Wins</td>
        </tr>
    }
    </tbody>
</table>
